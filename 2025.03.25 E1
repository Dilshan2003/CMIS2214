        int[] arr2={10,5,63,8,24,7,2,9,44,42};
        
        for(int i=0;i<arr2.length;i++){
            System.out.print(arr2[i]+" ");
        }
        
        int sum=0;
        for(int j=0;j<arr2.length;j++){
            sum+=arr2[j];
        }
        System.out.println("\nSum of all elements = "+sum);
        
        
        int max=arr2[0];
        for(int j=0;j<arr2.length;j++){
            if(arr2[j]>max){
            max=arr2[j];
            }
        }
        System.out.println("\nMaximun value in the array = "+max);
        
        
        int min=arr2[0];
        for(int j=0;j<arr2.length;j++){
            
            
 
             if(arr2[j]<min){
            min=arr2[j];
            }
        }
        System.out.println("\nMinimum value in the array = "+min);
        
    }
    
}
